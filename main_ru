meme_dict = {
    "КРИНЖ": "что-то очень странное или стыдное",
    "ЛОЛ": "что-то очень смешное",
    "РОФЛ": "шутка",
    "ЩИЩ": "одобрение или восторг",
    "КРИПОВЫЙ": "страшный/пугающий",
    "АГРИТЬСЯ": "злиться",
    "ОМГ" : "о боже!",
    "ВТФ" : "что за черт?",
    "ПРИВ" : "привет/здравствуй",
    "ОЧ" : "очень",
    "ОК" : "хорошо",
    "ДР" : "день Рожденья",
    "НГ" : "новый Год",
    "СПОК" : "спокойной ночи",
    "МБ" : "может быть",
    "ХЗ" : "хрен знает",
    "СПС" : "спасибо",
    "ИМХО" : "от английского 'in my humble opinion' По моему скромному мнению"
}

while True:
    word = input("Введите непонятное слово: ").upper()
    
    if word in meme_dict:
        meaning = meme_dict[word]
        print("Значение слова '{}': {}".format(word, meaning))
        
    elif word == 'СТОП':
        print('Спасибо, до свидания!')
        break
    
    elif word not in meme_dict:
        print('Слово', word, 'не поддерживается в словаре')
